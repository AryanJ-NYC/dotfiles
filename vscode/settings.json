// Place your settings in this file to overwrite the default settings
{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          //following will be in italic (=FlottFlott)
          "comment",
          "entity.name.type.class", //class names
          "keyword", //import, export, return…
          "constant", //String, Number, Boolean…, this, super
          "storage.modifier", //static keyword
          "storage.type.class.js" //class keyword
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          //following will be excluded from italics (VSCode has some defaults for italics)
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "terminal.integrated.shell.osx": "zsh",
  "editor.minimap.enabled": false,
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "typescript": "tsx"
  },
  "files.autoSave": "onFocusChange",
  "workbench.editor.enablePreview": false,
  "workbench.settings.editor": "json",
  "[java]": {
    "editor.tabSize": 4
  },
  "java.home": "/Users/aryanjabbari/.asdf/installs/java/adopt-openjdk-14+36",
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  "[markdown]": {
    "editor.wordWrap": "on"
  },
  "[mdx]": {
    "editor.tabSize": 2,
    "editor.wordWrap": "on"
  },
  "[php]": {
    "editor.tabSize": 4
  },
  "editor.fontSize": 16,
  "window.zoomLevel": 0,
  "git.confirmSync": false,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "explorer.autoReveal": false,
  "javascript.preferences.quoteStyle": "single",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.quoteStyle": "single",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.alwaysShowStatus": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": true,
  "window.nativeTabs": true,
  "window.titleBarStyle": "native",
  "workbench.startupEditor": "newUntitledFile",
  "editor.formatOnSave": true,
  "editor.suggestSelection": "first",
  "editor.fontFamily": "'Victor Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "python.languageServer": "Microsoft",
  "python.jediEnabled": false,
  "typescript.suggest.completeFunctionCalls": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "smooth",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "workbench.colorTheme": "Atom One Dark",
  "editor.renameOnType": true,
  "workbench.iconTheme": "vscode-great-icons",
  "typescript.tsserver.log": "verbose",
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"/Users/aryanjabbari/.vscode/extensions/gabrielbb.vscode-lombok-1.0.1/server/lombok.jar\"",
